use std::str;

class Vec3 {
  new(self, x, y z) {
    self.x = x;
    self.y = y;
    self.z = z;
  }

  fn +(self, other) {
    ret Vec3(self.x + other.x, self.y + other.y, self.z + other.z);
  }
}

let v1 = Vec3(1, 2, 3);
let v2 = Vec3(4, 5, 6);
let v3 = v1 + v2;

println(str::concat("(", v3.x, ", ", v3.y, ", ", v3.z, ")"));
