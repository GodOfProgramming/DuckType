{
  "name": "ducktype-syntax-highlights",
  "publisher": "GodOfProgramming",
  "repository": "https://github.com/GodOfProgramming/DuckType",
  "displayName": "DuckType Syntax Highlights",
  "description": "Syntax highlights for DuckType",
  "version": "0.42.0",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "DuckType",
        "aliases": [
          "DuckType",
          "ducktype"
        ],
        "extensions": [
          ".dk"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "DuckType",
        "scopeName": "source.dk",
        "path": "./syntaxes/DuckType.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^3.14.0"
  }
}